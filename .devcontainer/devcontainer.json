{
	"image": "ghcr.io/githedgehog/dpdk-sys/dev-env:main-rust-pinned",
	"name": "dataplane-dev-env",
	"mounts": [
		"type=bind,source=/mnt/huge/2M,target=/mnt/huge/2M,type=bind",
		"type=bind,source=/mnt/huge/1G,target=/mnt/huge/1G,type=bind",
		"type=bind,source=/var/run/docker.sock,destination=/var/run/docker.sock"
	],
	"privileged": true,
	"containerEnv": {
		"PATH": "/bin:/lib/openjdk/bin"
	},
	"containerUser": "runner",
	"updateRemoteUserUID": true,
	"securityOpt": ["seccomp=unconfined"],
	"runArgs": [
		"--network=host",
		"--name=dataplane-dev-env"
	]
}
